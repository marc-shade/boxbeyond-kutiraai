{
  "name": "kutiraai-agentic-framework",
  "version": "2.0.0",
  "private": true,
  "homepage": "https://github.com/daniel-manickam/KutiraAI",
  "description": "Comprehensive agentic framework with 64+ agents, 178+ MCP services, and real-time monitoring",
  "author": {
    "name": "CodedThemes",
    "email": "codedthemes@gmail.com",
    "url": "https://codedthemes.com/"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:ci": "BROWSERSLIST_IGNORE_OLD_DATA=1 vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:server": "eslint --env node agentic-framework-server.js api-server-real.js ws-server.js server.js task-orchestration.js start-dev.js",
    "lint:compact": "eslint \"src/**/*.{js,jsx,ts,tsx}\" -f compact --quiet",
    "lint:server:compact": "eslint --env node agentic-framework-server.js api-server-real.js ws-server.js server.js task-orchestration.js start-dev.js -f compact --quiet",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md,json,yml,yaml}\"",
    "prettier:list": "prettier --check \"**/*.{js,jsx,ts,tsx,css,md,json,yml,yaml}\" --list-different --log-level warn",
    "check:style:compact": "npm run -s lint:compact && npm run -s lint:server:compact && npm run -s prettier:list",
    "browserslist:update": "npx update-browserslist-db@latest",
    "agentic:start": "./agentic-startup.sh",
    "agentic:server": "node agentic-framework-server.js",
    "agentic:backend": "node api-server-real.js",
    "agentic:full": "concurrently \"npm run agentic:server\" \"npm run agentic:backend\" \"npm start\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
